    Annotations: 
        rdfs:label "StandardHeader"
    
    EquivalentTo: 
        heds:HED_0000010
         and (heds:HED_0000102 some hed:HED_0010004)
         and (heds:HED_0000300 value "8.3.1")
    
    
Class: hed:HED_0010011

    Annotations: 
        dc:description "The HED standard schema is a hierarchically-organized vocabulary for annotating events and experimental structure. HED annotations consist of comma-separated tags drawn from this vocabulary. This vocabulary can be augmented by terms drawn from specialized library schema.",
        rdfs:label "StandardPrologue"
    
    EquivalentTo: 
        heds:HED_0000011
         and (heds:HED_0000102 some hed:HED_0010004)
    
    
Class: hed:HED_0010012

    Annotations: 
        dc:description "This schema is released under the Creative Commons Attribution 4.0 International and is a product of the HED Working Group. The DOI for the latest version of the HED standard schema is 10.5281/zenodo.7876037.",
        rdfs:label "StandardEpilogue"
    
    EquivalentTo: 
        heds:HED_0000012
         and (heds:HED_0000102 some hed:HED_0010004)
    
    
Class: hed:HED_0010400

    Annotations: 
        dc:description "SI unit multiple representing 10.",
        rdfs:label "deca"
    
    EquivalentTo: 
        hed:HED_0010008
         and (hed:HED_0010305 value 10.0f)
         and (hed:HED_0010311 value true)
    
    
Class: hed:HED_0011303

    Annotations: 
        dc:description "Value must be a valid numerical value.",
        rdfs:label "numericClass"
    
    EquivalentTo: 
        hed:HED_0010009
         and (hed:HED_0010304 value "E")
         and (hed:HED_0010304 value "digits")
         and (hed:HED_0010304 value "e")
         and (hed:HED_0010304 value "hyphen")
         and (hed:HED_0010304 value "period")
         and (hed:HED_0010304 value "plus")
    
    
Class: hed:HED_0011500

    Annotations: 
        rdfs:label "accelerationUnits"
    
    EquivalentTo: 
        hed:HED_0010006
         and (hed:HED_0010104 some hed:HED_0011600)
    
    
Class: hed:HED_0011600

    Annotations: 
        rdfs:label "m-per-s^2"
    
    EquivalentTo: 
        hed:HED_0010007
         and (heds:HED_0000103 some hed:HED_0011500)
         and (hed:HED_0010304 value "caret")
         and (hed:HED_0010305 value 1.0)
         and (hed:HED_0010310 value true)
         and (hed:HED_0010317 value true)
    
    
Class: hed:HED_0012001

    Annotations: 
        dc:description "Something that happens at a given time and (typically) place. Elements of this tag subtree designate the general category in which an event falls.",
        rdfs:label "Event"
    
    EquivalentTo: 
        heds:HED_0000005
         and (heds:HED_0000102 some hed:HED_0010004)
    
    
Class: hed:HED_0012002

    Annotations: 
        dc:description "Something perceivable by the participant. An event meant to be an experimental stimulus should include the tag Task-property/Task-event-role/Experimental-stimulus.",
        rdfs:label "Sensory-event"
    
    SubClassOf: 
        hed:HED_0012001
    
    
Class: hed:HED_0012412

    Annotations: 
        dc:description "Something that pertains to a thing. A characteristic of some entity. A quality or feature regarded as a characteristic or inherent part of someone or something. HED attributes are adjectives or adverbs.",
        rdfs:label "Property"
    
    EquivalentTo: 
        heds:HED_0000005
         and (heds:HED_0000102 some hed:HED_0010004)
         and (hed:HED_0010307 value true)
    
    
Class: hed:HED_0012459

    Annotations: 
        dc:description "Designation of the type of a data item.",
        rdfs:label "Data-value"
    
    SubClassOf: 
        hed:HED_0012492
    
    
Class: hed:HED_0012492

    Annotations: 
        dc:description "Something that pertains to data or information.",
        rdfs:label "Data-property"
    
    SubClassOf: 
        hed:HED_0012412
    
    
Class: hed:HED_0012697

    Annotations: 
        dc:description "A value based on or employing the principles of statistics.",
        rdfs:label "Statistical-value"
    
    SubClassOf: 
        hed:HED_0012492
    
    
Class: hed:HED_0012698

    Annotations: 
        dc:description "The largest possible quantity or degree.",
        rdfs:label "Data-maximum"
    
    SubClassOf: 
        hed:HED_0012697
    
    
Class: hed:HED_0012699

    Annotations: 
        dc:description "The numeric value of the data maximum.",
        heds:HED_0010503 true,
        rdfs:label "Data-maximum-#"
    
    EquivalentTo: 
        hed:HED_0012698
         and (hed:HED_0010108 some hed:HED_0011303)
    
    
Class: heds:HED_0000001

    Annotations: 
        dc:description "Any item in the HED ontology.",
        rdfs:comment "Used to anchor all items in both the structural schema and the tags.",
        rdfs:label "HedEntity"
    
    SubClassOf: 
        owl:Thing
    
    
Class: heds:HED_0000002

    Annotations: 
        dc:description "Any structural element of the HED schema including header, sections, epilogue, and prologue.",
        rdfs:comment "Used to anchor all items in both the structural.",
        rdfs:label "HedStructure"
    
    EquivalentTo: 
        heds:HED_0000001
         and (heds:HED_0000102 exactly 1 heds:HED_0000004)
    
    
Class: heds:HED_0000003

    Annotations: 
        dc:description "Any item in a HED schema.",
        rdfs:comment "Used to anchor all items in both the structural schema and the tags.",
        rdfs:label "HedElement"
    
    EquivalentTo: 
        heds:HED_0000001
         and (heds:HED_0000102 exactly 1 heds:HED_0000004)
    
    
Class: heds:HED_0000004

    Annotations: 
        dc:description "The overall structure of the HED schema by section.",
        rdfs:label "HedSchema"
    
    SubClassOf: 
        heds:HED_0000002
    
    
Class: heds:HED_0000005

    Annotations: 
        dc:description "A HED vocabulary term or node representing a HED tag.",
        rdfs:label "HedTag"
    
    SubClassOf: 
        heds:HED_0000003
    
    
Class: heds:HED_0000006

    Annotations: 
        dc:description "A category of units such as acceleration units.",
        rdfs:label "HedUnitClass"
    
    EquivalentTo: 
        heds:HED_0000003
         and (heds:HED_0000102 exactly 1 heds:HED_0000004)
    
    
Class: heds:HED_0000007

    Annotations: 
        dc:description "A unit element such as seconds.",
        rdfs:comment "HED units have a isOfUnitType association with a HedUnitClass.",
        rdfs:label "HedUnit"
    
    EquivalentTo: 
        heds:HED_0000003
         and (heds:HED_0000103 exactly 1 heds:HED_0000006)
    
    
Class: heds:HED_0000008

    Annotations: 
        dc:description "A unit modifier such as kilo.",
        rdfs:label "HedUnitModifier"
    
    SubClassOf: 
        heds:HED_0000003
    
    
Class: heds:HED_0000009

    Annotations: 
        dc:description "Rules for a type of value provided by users to substitute for placeholders.",
        rdfs:label "HedValueClass"
    
    SubClassOf: 
        heds:HED_0000003
    
    
Class: heds:HED_0000010

    Annotations: 
        dc:description "Contains the specification of the schema version and partnership.",
        rdfs:label "HedHeader"
    
    EquivalentTo: 
        heds:HED_0000002
         and (heds:HED_0000300 exactly 1 xsd:string)
         and (heds:HED_0000301 max 1 xsd:string)
         and (heds:HED_0000302 max 1 xsd:boolean)
         and (heds:HED_0000303 max 1 xsd:string)
    
    
Class: heds:HED_0000011

    Annotations: 
        dc:description "The introductory text of a HED schema which describes is purpose and scope.",
        rdfs:label "HedPrologue"
    
    SubClassOf: 
        heds:HED_0000002
    
    
