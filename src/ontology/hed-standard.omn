    Annotations:
        rdfs:label "StandardUnitClass"

    EquivalentTo:
        heds:HED_0000006
         and (heds:HED_0000102 some hed:HED_0010004)

Class: hed:HED_0010007

    Annotations:
        rdfs:label "StandardUnit"

    EquivalentTo:
        heds:HED_0000007
         and (heds:HED_0000102 some hed:HED_0010004)

Class: hed:HED_0010008

    Annotations:
        rdfs:label "StandardUnitModifier"

    EquivalentTo:
        heds:HED_0000008
         and (heds:HED_0000102 some hed:HED_0010004)

