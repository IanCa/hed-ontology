        heds:HED_0000007
    
    Range: 
        xsd:boolean
    
    
Class: hed:HED_0010004

    Annotations: 
        rdfs:label "StandardSchema"
    
    EquivalentTo: 
        heds:HED_0000004
         and (heds:HED_0000101 some hed:HED_0010010)
         and (heds:HED_0000101 some hed:HED_0010011)
         and (heds:HED_0000101 some hed:HED_0010012)

